{
  "name": "earnpro-frontend",
  "version": "1.0.0",
  "description": "EarnPro Frontend - Professional earning platform",
  "main": "index.html",
  "scripts": {
    "dev": "live-server --port=8080 --host=localhost",
    "build": "npm run minify-css && npm run minify-js",
    "minify-css": "cleancss -o dist/css/main.min.css css/main.css css/auth.css css/dashboard.css css/mobile.css",
    "minify-js": "uglifyjs js/*.js -o dist/js/app.min.js",
    "deploy": "npm run build && vercel --prod",
    "lint": "eslint js/",
    "lint:fix": "eslint js/ --fix"
  },
  "keywords": ["frontend", "html", "css", "javascript", "earning-platform", "mobile-first"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.2",
    "clean-css-cli": "^5.6.2",
    "uglify-js": "^3.17.4",
    "eslint": "^8.56.0"
  },
  "browserslist": ["> 1%", "last 2 versions", "not dead"]
}
